        android:orientation="vertical" style="@style/groupStyleyellow_100">
        <ImageView android:id="@+id/imageHomeEleven"
            android:layout_gravity="end"
            android:layout_height="@dimen/_59pxh"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="@dimen/_59pxh"
            android:scaleType="fitXY" imageRes="@{@drawable/img_home11}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_home11"/>
        <LinearLayout android:id="@+id/linearColumnellipse779"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_100pxv"
            android:layout_marginEnd="@dimen/_6pxh"
            android:layout_marginStart="@dimen/_5pxh"
            android:layout_marginTop="@dimen/_59pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStackellipse779"
                android:layout_gravity="start"
                android:layout_height="@dimen/_248pxv" android:layout_width="@dimen/_379pxh">
                <View
                    android:background="@drawable/rectangle_bg_blue_100_radius_189_5"
                    android:id="@+id/viewEllipse779"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_248pxv" android:layout_width="@dimen/_379pxh"/>
