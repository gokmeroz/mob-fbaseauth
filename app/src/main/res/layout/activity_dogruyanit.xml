                    android:id="@+id/txtPrice"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_40pxh"
                    android:layout_marginVertical="@dimen/_40pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:text="@{dogruyanitVM.dogruyanitModel.txtPrice}"
                    style="@style/txtABeeZeeitalic19_1" tools:text="@string/msg_tebr_kler_do"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowellipse780"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnellipse780"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.61"
                    android:layout_width="0dp" android:orientation="vertical">
                    <View
                        android:background="@drawable/rectangle_bg_blue_100_radius_30"
                        android:id="@+id/viewEllipse780"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_59pxh"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:layout_marginStart="@dimen/_118pxh" android:layout_width="@dimen/_60pxh"/>
                    <View
                        android:background="@drawable/rectangle_bg_blue_100_radius_19"
                        android:id="@+id/viewEllipse781"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_38pxh"
                        android:layout_marginEnd="@dimen/_43pxh"
                        android:layout_marginStart="@dimen/_43pxh"
                        android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_38pxh"/>
                    <ImageView android:gravity="start"
                        android:id="@+id/imageEasterbunnyOne"
                        android:layout_height="@dimen/_224pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_easterbunny1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_easterbunny1"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_253pxv"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_33pxv"
                    android:layout_weight="0.18"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{dogruyanitVM.dogruyanitModel.txtOne}"
                    style="@style/txt_1" tools:text="@string/lbl_13"/>
                <ImageView android:id="@+id/imageMoneyOne"
                    android:layout_height="@dimen/_61pxh"
                    android:layout_marginBottom="@dimen/_253pxv"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="@dimen/_65pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_money2}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_money2"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
