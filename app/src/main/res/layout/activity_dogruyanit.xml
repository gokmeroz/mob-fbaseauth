<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dogruyanitVM" type="com.mob.app.modules.dogruyanit.data.viewmodel.DogruyanitVM"/>
    </data>
    <LinearLayout android:id="@+id/linearDogruYanit"
